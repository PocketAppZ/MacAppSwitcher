{
	"configurations": [
		{
			"type": "lldb-mi",
			"request": "launch",
			"name": "LLDB launch dbg",
			"target": "${workspaceFolder}/Output/Debug/MacAppSwitcher.exe",
			"cwd": "${workspaceFolder}/Output/Debug/",
			"valuesFormatting": "parseText",
			"preLaunchTask": "Build debug"
		},
		{
			"type": "lldb-mi",
			"request": "attach",
			"executable": "${workspaceFolder}/Output/Debug/MacAppSwitcher.exe",
			"name": "Attach dbg",
			"target": "[PID]",
			"cwd": "${workspaceRoot}",
			"valuesFormatting": "parseText"
		}
	]
}	